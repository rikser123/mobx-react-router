{
  "name": "mobx-react-router-simple",
  "version": "1.0.1",
  "description": "A react router base on mobx",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "test/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prettier": "npx prettier --write \"**/*.{js,jsx,ts,tsx,json,html,vue}\"",
    "eslint": "npx eslint --fix . --ext .ts",
    "test": "jest --no-cache --testTimeout=20000 --forceExit --all 'src/'",
    "build": "rimraf dist && tsc",
    "release": "standard-version -t ''",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rikser123/mobx-react-router"
  },
  "keywords": [
    "rikser123",
    "mobx",
    "router",
    "react"
  ],
  "author": "rikser123 <rikser123@yandex.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rikser123/mobx-react-router/issues"
  },
  "homepage": "https://github.com/rikser123/mobx-react-router#readme",
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "mobx": "^6.x",
    "mobx-react": "^9.x",
    "react": "^18.x",
    "react-dom": "^18.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mobx": "^6.11.0",
    "mobx-react": "^9.1.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npx eslint --fix --no-error-on-unmatched-pattern",
      "npx prettier --write"
    ]
  }
}
